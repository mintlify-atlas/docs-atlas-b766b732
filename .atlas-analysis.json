    "Build MCP Servers with tool, resource, and prompt support",
    "Create MCP Apps with interactive React widgets for ChatGPT/Claude",
    "Connect AI agents to multiple MCP servers",
    "Built-in inspector for testing and debugging",
    "OAuth authentication and authorization",
    "Notifications and sampling support",
    "Hot module reloading for development",
    "TypeScript and Python SDK implementations",
    "LangChain integration for agent building",
    "Streamable HTTP and WebSocket transports"
  ],
  "publicApiSurface": [
    "MCPServer - Server framework class",
    "MCPClient - Client for connecting to servers",
    "MCPAgent - Agent framework with LLM integration",
    "server.tool() - Register tools",
    "server.resource() - Register resources",
    "server.prompt() - Register prompts",
    "server.widget() - Register UI widgets",
    "server.listen() - Start server with inspector",
    "client.createAllSessions() - Initialize connections",
    "agent.run() - Execute agent tasks",
    "agent.stream() - Streaming responses",
    "useMcp() - React hook for MCP integration",
    "useWidget() - React hook for widget props",
    "text(), widget(), resource() - Response helpers",
    "oauthSupabaseProvider() - OAuth integration",
    "FileSystemSessionStore - Session persistence",
    "@mcp-use/cli - Development CLI",
    "@mcp-use/inspector - Web-based debugger",
